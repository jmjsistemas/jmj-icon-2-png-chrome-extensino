{ "manifest_version": 2,
  "name": "JMJ Icon2PNG",
  "description": "Copy icon!",
  "version": "4.0",

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": [  "*://*/*","file:///*","https://www.flaticon.com/*"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_title": "JMJ Icon2PNG"
  },

  "permissions": ["activeTab"]
}